<UserControl x:Class="GPAS.Workspace.Presentation.Applications.DataImportApplication"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GPAS.Workspace.Presentation.Applications"
             xmlns:dataImport="clr-namespace:GPAS.Workspace.Presentation.Controls.DataImport"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <dataImport:DataImportUserControl x:Name="DataImportUserControl" 
                                          AddFiles="DataImportUserControlOnAddFiles"
                                          RemoveDataSources="DataImportUserControlOnRemoveDataSources"
                                          DataSourceListSelectionChanged="DataImportUserControlOnDataSourceListSelectionChanged"
                                          ShowOnGraphRequested="DataImportUserControl_ShowOnGraphRequested"/>
    </Grid>
</UserControl>
