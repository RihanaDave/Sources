<UserControl x:Class="GPAS.Workspace.Presentation.Controls.DataImport.BindableMappingGraphUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:graphViewer="clr-namespace:GPAS.Graph.GraphViewer;assembly=GPAS.Graph.GraphViewer"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Grid>
        <graphViewer:GraphViewer x:Name="MainGraphViewer" 
                                 Foreground="{DynamicResource MaterialDesignBody}"
                                 ViewerInitializationCompleted="MainGraphViewerOnViewerInitializationCompleted"/>
    </Grid>
</UserControl>
